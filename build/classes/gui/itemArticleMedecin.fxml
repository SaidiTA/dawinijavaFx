<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" prefHeight="105.0" prefWidth="925.0" style="-fx-border-image-insets: 0; -fx-background-color: #F1F7FD;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ItemArticleMedecinController">
   <children>
      <HBox alignment="CENTER" prefHeight="105.0" prefWidth="772.0">
         <children>
            <Label fx:id="btnid" alignment="CENTER" contentDisplay="CENTER" prefHeight="24.0" prefWidth="0.0" text="">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lblName" alignment="CENTER" contentDisplay="CENTER" prefHeight="24.0" prefWidth="290.0" text="">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lbldescription" alignment="CENTER" contentDisplay="CENTER" prefHeight="24.0" prefWidth="319.0">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lblDate" alignment="CENTER" contentDisplay="CENTER" prefHeight="24.0" prefWidth="231.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
     <HBox fx:id="lblAction" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="7.0">
         <children>
            <Button fx:id="btnshow" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#Affiche" prefHeight="40.0" prefWidth="45.0" style="-fx-background-color: #70B8D9;" stylesheets="@style.css">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/1814102_eye_sight_view_icon.png" />
                     </image>
                  </ImageView>
               </graphic>
               <opaqueInsets>
                  <Insets top="3.0" />
               </opaqueInsets>
               <HBox.margin>
                  <Insets top="3.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnModify" alignment="BASELINE_LEFT" contentDisplay="CENTER" mnemonicParsing="false" onAction="#Modifier" prefHeight="40.0" prefWidth="45.0" style="-fx-background-color: #EBC95E;" stylesheets="@style.css">
               <graphic>
                  <ImageView fitHeight="29.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/1976055_edit_edit%20document_edit%20file_edited_editing_icon.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="btnDelete" alignment="BASELINE_LEFT" contentDisplay="CENTER" mnemonicParsing="false" onAction="#supprimer" prefHeight="40.0" prefWidth="45.0" style="-fx-background-color: #E37A7A;" stylesheets="@style.css">
               <graphic>
                  <ImageView fitHeight="26.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/tr-removebg-preview.png" />
                     </image>
                  </ImageView>
               </graphic>
               <HBox.margin>
                  <Insets right="7.0" />
               </HBox.margin>
            </Button>
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets bottom="40.0" />
   </opaqueInsets>
</HBox>
